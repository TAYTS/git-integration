    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.180",
    "@types/node": "^16.11.1",
    "@types/passport": "^1.0.7",
    "@types/passport-http": "^0.3.9",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
