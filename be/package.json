    "@nestjs/core": "^8.1.1",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.1.1",
    "@prisma/client": "^3.10.0",
    "bcrypt": "^5.0.1",
    "dotenv-flow-cli": "^1.0.0",
    "ioredis": "^4.28.5",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.1",
    "passport": "^0.5.2",
    "passport-http": "^0.3.0",
